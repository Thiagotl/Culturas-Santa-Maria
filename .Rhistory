geom_line(aes(y = Soja, color = "Soja")) +
labs (title = "Produção de Arroz e Soja nos anos de 1974 a 2020 - Santa Maria/RS",
x = "Anos",
y = "Quantidade (t)",
color = "Culturas") +
theme(axis.text.x=element_text(angle=60, hjust=1))
